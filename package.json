{
  "name": "exquisite_corpse",
  "version": "1.0.0",
  "description": "a collaborative drawing app",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "server:debug": "nodemon --inspect index.js",
    "frontend-install": "npm install --prefix client",
    "frontend-install-prod": "npm install --silent --prefix client",
    "frontend": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "dev:debug": "concurrently \"npm run server:debug \" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "aliao3511",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.562.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.7",
    "multer": "^1.4.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-redux": "^7.1.1",
    "socket.io": "^2.4.0",
    "validator": "^12.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  },
  "engines": {
    "node": "12.14.1",
    "npm": "6.13.4"
  }
}
